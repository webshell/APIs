		if (url.indexOf('[/files]') !== -1) {
			url = url.replace('[/files]', '/' + args['files'])
			if (args['files']) delete args['files']
		}
		if (url.indexOf('[/id]') !== -1) url = url.replace('[/id]', '')
		if (url.indexOf('[/description]') !== -1) url = url.replace('[/description]', '')
		if (url.indexOf('[/public]') !== -1) url = url.replace('[/public]', '')
		if (url.indexOf('[/files]') !== -1) url = url.replace('[/files]', '')
		return http({provider:'github',url:url,method:'PATCH',params:args, auth:'oauth2'}, opts)
	},
