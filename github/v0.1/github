		if (url.indexOf('[/repo]') !== -1) url = url.replace('[/repo]', '')
		if (url.indexOf('[/id]') !== -1) url = url.replace('[/id]', '')
		return http({provider:'github',url:url,method:'DELETE',params:args, auth:'oauth2'}, opts)
	},
