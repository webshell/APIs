		if (url.match(rgx)) {
			url = url.replace(rgx, args['has_issues'])
			if (args['has_issues']) delete args['has_issues']
		}
		if (url.indexOf('[/has_issues]') !== -1) {
			url = url.replace('[/has_issues]', '/' + args['has_issues'])
			if (args['has_issues']) delete args['has_issues']
		}
		var rgx = new RegExp('\{has_wiki\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['has_wiki'])
			if (args['has_wiki']) delete args['has_wiki']
		}
		if (url.indexOf('[/has_wiki]') !== -1) {
			url = url.replace('[/has_wiki]', '/' + args['has_wiki'])
			if (args['has_wiki']) delete args['has_wiki']
		}
		var rgx = new RegExp('\{has_downloads\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['has_downloads'])
			if (args['has_downloads']) delete args['has_downloads']
		}
		if (url.indexOf('[/has_downloads]') !== -1) {
			url = url.replace('[/has_downloads]', '/' + args['has_downloads'])
			if (args['has_downloads']) delete args['has_downloads']
		}
		var rgx = new RegExp('\{team_id\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['team_id'])
			if (args['team_id']) delete args['team_id']
		}
		if (url.indexOf('[/team_id]') !== -1) {
			url = url.replace('[/team_id]', '/' + args['team_id'])
			if (args['team_id']) delete args['team_id']
		}
		if (url.indexOf('[/name]') !== -1) url = url.replace('[/name]', '')
		if (url.indexOf('[/description]') !== -1) url = url.replace('[/description]', '')
		if (url.indexOf('[/homepage]') !== -1) url = url.replace('[/homepage]', '')
		if (url.indexOf('[/private]') !== -1) url = url.replace('[/private]', '')
		if (url.indexOf('[/has_issues]') !== -1) url = url.replace('[/has_issues]', '')
		if (url.indexOf('[/has_wiki]') !== -1) url = url.replace('[/has_wiki]', '')
		if (url.indexOf('[/has_downloads]') !== -1) url = url.replace('[/has_downloads]', '')
		if (url.indexOf('[/team_id]') !== -1) url = url.replace('[/team_id]', '')
		return http({provider:'github',url:url,method:'POST',params:args, auth:'oauth2'}, opts)
	},
