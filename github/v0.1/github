			url = url.replace('[/parents]', '/' + args['parents'])
			if (args['parents']) delete args['parents']
		}
		var rgx = new RegExp('\{author.name\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['author.name'])
			if (args['author.name']) delete args['author.name']
		}
		if (url.indexOf('[/author.name]') !== -1) {
			url = url.replace('[/author.name]', '/' + args['author.name'])
			if (args['author.name']) delete args['author.name']
		}
		var rgx = new RegExp('\{author.email\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['author.email'])
			if (args['author.email']) delete args['author.email']
		}
		if (url.indexOf('[/author.email]') !== -1) {
			url = url.replace('[/author.email]', '/' + args['author.email'])
			if (args['author.email']) delete args['author.email']
		}
		var rgx = new RegExp('\{author.date\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['author.date'])
			if (args['author.date']) delete args['author.date']
		}
		if (url.indexOf('[/author.date]') !== -1) {
			url = url.replace('[/author.date]', '/' + args['author.date'])
			if (args['author.date']) delete args['author.date']
		}
		var rgx = new RegExp('\{committer.name\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['committer.name'])
			if (args['committer.name']) delete args['committer.name']
		}
		if (url.indexOf('[/committer.name]') !== -1) {
			url = url.replace('[/committer.name]', '/' + args['committer.name'])
			if (args['committer.name']) delete args['committer.name']
		}
		var rgx = new RegExp('\{committer.email\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['committer.email'])
			if (args['committer.email']) delete args['committer.email']
		}
		if (url.indexOf('[/committer.email]') !== -1) {
			url = url.replace('[/committer.email]', '/' + args['committer.email'])
			if (args['committer.email']) delete args['committer.email']
		}
		var rgx = new RegExp('\{committer.date\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['committer.date'])
			if (args['committer.date']) delete args['committer.date']
		}
		if (url.indexOf('[/committer.date]') !== -1) {
			url = url.replace('[/committer.date]', '/' + args['committer.date'])
			if (args['committer.date']) delete args['committer.date']
		}
		if (url.indexOf('[/user]') !== -1) url = url.replace('[/user]', '')
		if (url.indexOf('[/repo]') !== -1) url = url.replace('[/repo]', '')
		if (url.indexOf('[/message]') !== -1) url = url.replace('[/message]', '')
		if (url.indexOf('[/tree]') !== -1) url = url.replace('[/tree]', '')
		if (url.indexOf('[/parents]') !== -1) url = url.replace('[/parents]', '')
		if (url.indexOf('[/author.name]') !== -1) url = url.replace('[/author.name]', '')
		if (url.indexOf('[/author.email]') !== -1) url = url.replace('[/author.email]', '')
		if (url.indexOf('[/author.date]') !== -1) url = url.replace('[/author.date]', '')
		if (url.indexOf('[/committer.name]') !== -1) url = url.replace('[/committer.name]', '')
		if (url.indexOf('[/committer.email]') !== -1) url = url.replace('[/committer.email]', '')
		if (url.indexOf('[/committer.date]') !== -1) url = url.replace('[/committer.date]', '')
		return http({provider:'github',url:url,method:'POST',params:args, auth:'oauth2'}, opts)
	},
	'getReference': function(args, opts) {
