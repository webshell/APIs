		if (url.indexOf('[/name]') !== -1) {
			url = url.replace('[/name]', '/' + args['name'])
			if (args['name']) delete args['name']
		}
		var rgx = new RegExp('\{permission\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['permission'])
			if (args['permission']) delete args['permission']
		}
		if (url.indexOf('[/permission]') !== -1) {
			url = url.replace('[/permission]', '/' + args['permission'])
			if (args['permission']) delete args['permission']
		}
		if (url.indexOf('[/id]') !== -1) url = url.replace('[/id]', '')
		if (url.indexOf('[/name]') !== -1) url = url.replace('[/name]', '')
		if (url.indexOf('[/permission]') !== -1) url = url.replace('[/permission]', '')
		return http({provider:'github',url:url,method:'PATCH',params:args, auth:'oauth2'}, opts)
	},
