	'getBlob': function(args, opts) {
		var url = 'https://api.github.com/repos/{user}/{repo}/git/blobs/{sha}'
		args = args || {}
		if (args['user'] == null) throw new Error('user is a required parameter')
		var rgx = new RegExp('\{user\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['user'])
			if (args['user']) delete args['user']
		}
		if (url.indexOf('[/user]') !== -1) {
			url = url.replace('[/user]', '/' + args['user'])
			if (args['user']) delete args['user']
		}
		if (args['repo'] == null) throw new Error('repo is a required parameter')
		var rgx = new RegExp('\{repo\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['repo'])
			if (args['repo']) delete args['repo']
		}
		if (url.indexOf('[/repo]') !== -1) {
			url = url.replace('[/repo]', '/' + args['repo'])
			if (args['repo']) delete args['repo']
		}
		if (args['sha'] == null) throw new Error('sha is a required parameter')
		var rgx = new RegExp('\{sha\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['sha'])
			if (args['sha']) delete args['sha']
		}
		if (url.indexOf('[/sha]') !== -1) {
			url = url.replace('[/sha]', '/' + args['sha'])
			if (args['sha']) delete args['sha']
		}
		if (url.indexOf('[/user]') !== -1) url = url.replace('[/user]', '')
		if (url.indexOf('[/repo]') !== -1) url = url.replace('[/repo]', '')
		if (url.indexOf('[/sha]') !== -1) url = url.replace('[/sha]', '')
		return http({provider:'github',url:url,method:'GET',params:args, auth:'none'}, opts)
	},
	'createBlob': function(args, opts) {
		var url = 'https://api.github.com/repos/{user}/{repo}/git/blobs'
		args = args || {}
		if (args['user'] == null) throw new Error('user is a required parameter')
		var rgx = new RegExp('\{user\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['user'])
			if (args['user']) delete args['user']
		}
		if (url.indexOf('[/user]') !== -1) {
			url = url.replace('[/user]', '/' + args['user'])
			if (args['user']) delete args['user']
		}
		if (args['repo'] == null) throw new Error('repo is a required parameter')
		var rgx = new RegExp('\{repo\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['repo'])
			if (args['repo']) delete args['repo']
		}
		if (url.indexOf('[/repo]') !== -1) {
			url = url.replace('[/repo]', '/' + args['repo'])
			if (args['repo']) delete args['repo']
		}
		if (args['content'] == null) throw new Error('content is a required parameter')
		var rgx = new RegExp('\{content\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['content'])
			if (args['content']) delete args['content']
		}
		if (url.indexOf('[/content]') !== -1) {
			url = url.replace('[/content]', '/' + args['content'])
			if (args['content']) delete args['content']
		}
		if (args['encoding'] == null) args['encoding'] = "utf-8"
		var rgx = new RegExp('\{encoding\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['encoding'])
			if (args['encoding']) delete args['encoding']
		}
		if (url.indexOf('[/encoding]') !== -1) {
			url = url.replace('[/encoding]', '/' + args['encoding'])
			if (args['encoding']) delete args['encoding']
		}
		if (url.indexOf('[/user]') !== -1) url = url.replace('[/user]', '')
		if (url.indexOf('[/repo]') !== -1) url = url.replace('[/repo]', '')
		if (url.indexOf('[/content]') !== -1) url = url.replace('[/content]', '')
		if (url.indexOf('[/encoding]') !== -1) url = url.replace('[/encoding]', '')
		return http({provider:'github',url:url,method:'POST',params:args, auth:'oauth2'}, opts)
	},
