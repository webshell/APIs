			url = url.replace('[/assignee]', '/' + args['assignee'])
			if (args['assignee']) delete args['assignee']
		}
		var rgx = new RegExp('\{state\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['state'])
			if (args['state']) delete args['state']
		}
		if (url.indexOf('[/state]') !== -1) {
			url = url.replace('[/state]', '/' + args['state'])
			if (args['state']) delete args['state']
		}
		var rgx = new RegExp('\{milestone\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['milestone'])
			if (args['milestone']) delete args['milestone']
		}
		if (url.indexOf('[/milestone]') !== -1) {
			url = url.replace('[/milestone]', '/' + args['milestone'])
			if (args['milestone']) delete args['milestone']
		}
		var rgx = new RegExp('\{labels\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['labels'])
			if (args['labels']) delete args['labels']
		}
		if (url.indexOf('[/labels]') !== -1) {
			url = url.replace('[/labels]', '/' + args['labels'])
			if (args['labels']) delete args['labels']
		}
		if (url.indexOf('[/user]') !== -1) url = url.replace('[/user]', '')
		if (url.indexOf('[/repo]') !== -1) url = url.replace('[/repo]', '')
		if (url.indexOf('[/number]') !== -1) url = url.replace('[/number]', '')
		if (url.indexOf('[/title]') !== -1) url = url.replace('[/title]', '')
		if (url.indexOf('[/body]') !== -1) url = url.replace('[/body]', '')
		if (url.indexOf('[/assignee]') !== -1) url = url.replace('[/assignee]', '')
		if (url.indexOf('[/state]') !== -1) url = url.replace('[/state]', '')
		if (url.indexOf('[/milestone]') !== -1) url = url.replace('[/milestone]', '')
		if (url.indexOf('[/labels]') !== -1) url = url.replace('[/labels]', '')
		return http({provider:'github',url:url,method:'PATCH',params:args, auth:'oauth2'}, opts)
	},
