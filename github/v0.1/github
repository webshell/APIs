	url = url_replace(url, args, 'repo', 'str', true);
	url = url_replace(url, args, 'archive_format', 'str', true);
	url = url_replace(url, args, 'ref', 'str', false, "master");
	return http({provider:provider,url:url,params:args, auth:'none'}, opts);
};

