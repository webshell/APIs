	var url = 'https://api.deezer.com/2.0/album/{id}/comments';
	args = args || {};
	url = url_replace(url, args, 'id', 'str', true);
	url = url_replace(url, args, 'comment', 'str', true);
	return http.post({provider:provider,url:url,params:args, auth:'oauth2'}, opts);
};

