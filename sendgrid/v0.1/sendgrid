		if (url.indexOf('[/zip]') !== -1) url = url.replace('[/zip]', '')
		if (url.indexOf('[/country]') !== -1) url = url.replace('[/country]', '')
		return http({provider:'sendgrid',url:url,method:'GET',params:args}, opts)
