			url = url.replace('[/trim_user]', '/' + args['trim_user'])
			if (args['trim_user']) delete args['trim_user']
		}
		var rgx = new RegExp('\{exclude_replies\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['exclude_replies'])
