exports.getMeGroups = function(args, opts) {
	var url = 'https://api.soundcloud.com/me/groups.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', false, "json");
	return http({provider:provider,url:url,params:args, auth:'oauth2'}, opts);
};

