		if (args['method'] == null) args['method'] = "templateUndel"
		var rgx = new RegExp('\{method\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['method'])
			if (args['method']) delete args['method']
		}
		if (url.indexOf('[/method]') !== -1) {
			url = url.replace('[/method]', '/' + args['method'])
			if (args['method']) delete args['method']
		}
