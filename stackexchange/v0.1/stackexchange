	url = url_replace(url, args, 'page', 'int');
	url = url_replace(url, args, 'pagesize', 'int');
	url = url_replace(url, args, 'site', 'str', true, "stackoverflow");
	return http({provider:provider,url:url,params:args}, opts);
};

