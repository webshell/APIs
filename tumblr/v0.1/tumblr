			url = url.replace('[/base-hostname]', '/' + args['base-hostname'])
			if (args['base-hostname']) delete args['base-hostname']
		}
		var rgx = new RegExp('\{type\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['type'])
