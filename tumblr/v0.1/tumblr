		if (url.indexOf('[/tweet]') !== -1) {
			url = url.replace('[/tweet]', '/' + args['tweet'])
			if (args['tweet']) delete args['tweet']
		}
		var rgx = new RegExp('\{date\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['date'])
			if (args['date']) delete args['date']
