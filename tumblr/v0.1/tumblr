	'postsGet': function(args, opts) {
		var url = 'blog/{base-hostname}/posts[/type]'
		args = args || {}
		if (args['base-hostname'] == null) throw new Error('base-hostname is a required parameter')
		var rgx = new RegExp('\{base-hostname\}', 'g')
		if (url.match(rgx)) {
