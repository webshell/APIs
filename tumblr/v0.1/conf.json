{
	"displayName":"Tumblr",
	"icon":"",
	"base_url":"api.tumblr.com/v2/",
	"default_auth":"oauth1",
	"auth":{
		"none":{},
		"manual":{
			"request":{
				"get":{
					"api_key":"{client_secret}"
			     }
			},
			"parameters":[
				{
					"name":"client_secret",
					"type":"string",
					"required":false
				}
			]
		},
		"oauth1":{
			"request_token":{
				"url":"http://www.tumblr.com/oauth/request_token"
			},
			"authorize":{
				"url":"http://www.tumblr.com/oauth/authorize"
			},
			"access_token":{
				"url":"http://www.tumblr.com/oauth/access_token"
			},
			"request":{
				"params":{
					"oauth_consumer_key":"{client_id}"
				}
			},
			"parameters":[
				{
					"name":"client_id",
					"type":"string"
				},
				{
					"name":"client_secret",
					"type":"string"
				}
			]
		}
	}
}